<UserControl x:Class="WPFTest.View.AView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPFTest.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Background="Orange"
             xmlns:convMVVM="https://github.com/gellston/ConvMVVM"
             convMVVM:DialogServiceViews.IsRegistered="True">
    <Grid Margin="50">
        <Button Command="{Binding TestCommand}"></Button>
    </Grid>
</UserControl>
