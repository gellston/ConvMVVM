<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Remove="Properties\Resources.en.resx" />
    <EmbeddedResource Remove="Properties\Resources.kr.resx" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="ConvMVVM.WPF" Version="1.0.10" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Properties\Resources.en.resx" />
    <Resource Include="Properties\Resources.kr.resx" />
  </ItemGroup>

</Project>
